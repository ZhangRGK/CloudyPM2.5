{
    "manifest_version": 2,
    "name": "我的时钟",
    "version": "1.0",
    "description": "我的第一个Chrome扩展",
    "icons": {
        "16": "images/blackkitty16.png",
        "48": "images/blackkitty48.png",
        "128": "images/blackkitty128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/blackkitty19.png",
            "38": "images/blackkitty38.png"
        },
        "default_title": "我的时钟",
        "default_popup": "popup.html"
    },
    "background":{"scripts":["background/bg.js"]},
    "permissions": [
        "https://*/",
        "http://*/",
        "geolocation"
    ]
}